<?xml version='1.0' encoding='utf-8'?>
<widget id="com.syngenta.syngentaua" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Сингента Україна</name>
    <description>
       Додаток для професіоналів аграрного ринку
    </description>
    <author email="syngenta.office@gmail.com" href="http://syngenta.ua">
      Oleksii Dolhodushev
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
